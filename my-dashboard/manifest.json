{
	"name": "my dashboard",
	"version": "0.0.1",
	"description": "My Dashboard",
	"action": {
		"default_icon": "images/128x128.png",
		"default_popup": "popup.html"
	},
   	"background": {
		"service_worker": "background.js",
		"type": "module"
	},

	"offline_enabled": true,
	"permissions" : [
		"notifications",
		"background",
		"tabs",
		"storage",
		"cookies"
	],
	"host_permissions": [
		"https://spectra.daouoffice.com/api/*",
		"https://enomix.atlassian.net/*"
	],
	"manifest_version": 3
}
