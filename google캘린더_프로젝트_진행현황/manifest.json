{
    "name" : "EER 프로젝트 진행현황",
	"browser_action": {
		"default_icon": "images/_calendar.png",
		"default_popup": "popup.html"
    },
    "version": "0.1.12",
    "manifest_version" : 2,
 
    "permissions" : [               
        "notifications",
		"http://*/*",
		"background",
		"storage",
		"tabs"
    ],
    "background": { "scripts": ["js/background.js", "js/jquery-1.9.1.min.js"] }
}