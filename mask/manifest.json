{
    "name" : "마스크 구입",
    "version": "1",
    "manifest_version" : 2,
    "permissions" : [
        "notifications",
		"http://*/*",
        "*://smartstore.naver.com/*",
		"background"
    ],
    "web_accessible_resources" : [
        "48.png"
    ],
    "content_security_policy": "script-src 'self' https://smartstore.naver.com; object-src 'self'",
    "background": { "scripts": ["background.js", "jquery-1.9.1.min.js", "jquery.ajaxQueue.min.js"] }
}


