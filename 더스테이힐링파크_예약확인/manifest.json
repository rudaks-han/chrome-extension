{
    "name" : "더스테이힐링파크 예약확인",
    "version": "1",
    "manifest_version" : 2,
 
    "permissions" : [               // 권한
        "notifications",
		"http://*/*",
        "*://live.ipms247.com/*",
		"background",
        "cookies",
        "storage"
    ],
 
    "web_accessible_resources" : [  // 알림창에 뜰 아이콘
        "48.png"
    ],
	
    "background": { "scripts": ["background.js", "jquery-1.9.1.min.js"] },
    "content_security_policy": "script-src 'self' https://live.ipms247.com 'unsafe-eval'; object-src 'self'"

    //"content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",

}


